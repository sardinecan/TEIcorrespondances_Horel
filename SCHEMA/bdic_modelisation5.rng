            </zeroOrMore>
            <zeroOrMore>
               <group>
                  <choice>
                     <ref name="tei_zone"/>
                     <ref name="tei_surface"/>
                     <ref name="tei_surfaceGrp"/>
                  </choice>
               </group>
               <zeroOrMore>
                  <ref name="tei_model.global"/>
               </zeroOrMore>
