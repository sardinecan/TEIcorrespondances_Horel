            <ref name="tei_data.pointer"/>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="tei_elementSpec">
      <element name="elementSpec">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(spécification d'élément) documente la structure, le contenu et l'emploi d'un élément. [22.4.4. Element Specifications 22. Documentation Elements]</a:documentation>
         <group>
            <zeroOrMore>
               <choice>
                  <ref name="tei_model.glossLike"/>
                  <ref name="tei_model.descLike"/>
               </choice>
